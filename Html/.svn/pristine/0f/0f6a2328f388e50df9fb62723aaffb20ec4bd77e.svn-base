.layer-end
{
    font-size: 30px;
    font-weight: 400;

    position: absolute;
    top: 10px;
    right: 7px;

    width: 30px;
    height: 30px;

    cursor: pointer;

    border: 0;
    background: transparent;

    -webkit-appearance: none;
}
.layer-end::before,
.layer-end::after
{
    position: absolute;
    top: 13px;
    left: 5px;

    width: 20px;
    height: 2px;

    content: '';
    -webkit-transform: rotate(45deg);
            transform: rotate(45deg);

    border-radius: 3px;
    background-color: rgba(0,0,0,.3);
}
.layer-end::after
{
    -webkit-transform: rotate(-45deg);
            transform: rotate(-45deg);
}
