/*复选同意框*/

.ui-checked
{
    line-height: 22px;

    position: relative;
    z-index: 2;

    width: 20px;
    height: 22px;

    vertical-align: sub;

    border: 0;
    outline: 0;
    background: 0 0;

    -webkit-appearance: none;
}

.ui-checked:before
{
    font-family: 'iconfont';
    font-size: 20px;
    line-height: 22px;

    top: 0;
    left: 0;

    display: inline-block;

    width: 20px;
    height: 22px;

    content: '\e615';
    cursor: pointer;

    color: #cacaca;
}

.ui-checked:checked:before
{
    color: #f60;
}

.ui-checked-round
{
    line-height: 28px;

    position: relative;
    z-index: 2;

    width: 28px;
    height: 28px;

    vertical-align: sub;

    border: 0;
    outline: 0;
    background: 0 0;

    -webkit-appearance: none;
}

.ui-checked-round:before
{
    font-family: 'iconfont';
    font-size: 28px;
    line-height: 28px;

    top: 0;
    left: 0;

    display: inline-block;

    width: 28px;
    height: 28px;

    content: '\e65a';
    cursor: pointer;

    color: #cacaca;
}

.ui-checked-round:checked:before
{
    color: #00aaef;
}


