.ui-select,
.ui-checked,
.ui-checked-round,
.ui-date,
.ui-day,
.ui-rmb,
.ui-per,
.ui-home,
.ui-adr,
.ui-people,
.ui-link,
.ui-contact
{
    position: relative;

    margin-right: 6px;
}

.ui-select select,
.ui-date input,
.ui-day input,
.ui-rmb input,
.ui-per input,
.ui-home input,
.ui-adr input,
.ui-people input,
.ui-link input,
.ui-contact input,
.fixed-form-cont-item input,
.my-regular-form-cont-item input
{
    width: 100%;
    padding-right: 16px;

    border: 0;
    background: 0 0;

    -webkit-appearance: none;
}

.ui-select:after,
.ui-date:after,
.ui-day:after,
.ui-rmb:after,
.ui-per:after,
.ui-home:after,
.ui-adr:after,
.ui-people:after,
.ui-people .iconfont,
.ui-link:after,
.ui-contact:after,
.fixed-form-cont-item:after,
.transfer-fixed-form-cont-item-up:after,
.my-regular-form-cont-item:after,
.ui-add:after
{
    font-family: 'iconfont' !important;
    font-size: 18px;
    font-style: normal;
    line-height: 18px;

    position: absolute;
    top: 50%;
    right: 0;

    width: 18px;
    height: 18px;
    margin-top: -9px;

    pointer-events: none;

    color: #3969ba;

    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
    -moz-osx-font-smoothing: grayscale;
}

.ui-select:after
{
    content: '\e604';
}

.ui-select.disabled:after
{
    content: '\e604';

    color: #ccc;
}

.ui-date:after
{
    content: '\e601';
}

.ui-day:after
{
    content: '\e620';

    color: #999;
}

.ui-rmb:after
{
    content: '\e621';

    color: #999;
}

.ui-per:after
{
    content: '\e622';

    color: #999;
}

.ui-home:after
{
    content: '\e60c';
}

.ui-adr:after
{
    content: '\e60b';
}

.ui-people:after
{
    content: '\e605';
}

.ui-link:after
{
    content: '\e600';
}

.ui-contact:after
{
    content: '\e61e';
}


