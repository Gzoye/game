}.layer-box
{
    position: absolute;
    z-index: 19891014;
    top: 0;
    left: 0;

    width: 100%;
}

.layer-shade,
.layer-main
{
    position: fixed;
    top: 0;
    left: 0;

    width: 100%;
    height: 100%;
}

.layer-main
{
    font-family: Helvetica, arial, sans-serif;

    display: table;

    pointer-events: none;
}

.layer-shade
{
    pointer-events: auto;

    background-color: rgba(0,0,0, .5);
}

.layer-main .section
{
    display: table-cell;

    text-align: center;
    vertical-align: middle;
}
.layer-child
{
    font-size: 14px;

    position: relative;

    display: inline-block;

    text-align: left;
    pointer-events: auto;

    border-radius: 6px;
    background-color: #fff;
    box-shadow: 0 0 8px rgba(0, 0, 0, .1);
}
.layer-child
{
    -webkit-animation-duration: .18s;
            animation-duration: .18s;

    -webkit-animation-fill-mode: both;
            animation-fill-mode: both;
}
.layer-border
{
    border: 1px solid #999;
}

@-webkit-keyframes bounceIn
{
    /* 默认 */
    0%
    {
        -webkit-transform: scale(.5);
                transform: scale(.5);

        opacity: 0;
    }
    100%
    {
        -webkit-transform: scale(1);
                transform: scale(1);

        opacity: 1;
    }
}
.layer-manim
{
    -webkit-animation-name: bounceIn;
            animation-name: bounceIn;
}


.layer-box0 .layer-child
{
    /*max-width:85%; min-width:85%;*/width: 85%;
}
.layer-box1 .layer-child
{
    border: none;
    border-radius: 0;
}
.layer-box2 .layer-child
{
    width: auto;
    min-width: 40px;
    max-width: 260px;

    color: #fff;
    border: none;
    background-color: rgba(0,0,0,.6);
}
.layer-child h3
{
    font-size: 16px;
    font-weight: 400;
    line-height: 45px;

    height: 45px;
    padding: 0 45px 0 10px;

    border-bottom: 1px solid #ebebeb;
    border-bottom: 1px solid #ebebeb;
    border-radius: 5px 5px 0 0;
}



.layer-cont
{
    line-height: 16px;

    padding: 20px 15px;

    border-radius: 5px;
}

.layer-btn
{
    font-size: 0;
    line-height: 40px;

    position: relative;

    height: 40px;

    text-align: center;

    border-top: 1px solid #ebebeb;
}
.layer-btn span
{
    font-size: 14px;

    position: relative;

    display: inline-block;

    width: 50%;

    cursor: pointer;
    text-align: center;

    border-radius: 0 5px 0 0;
}
.layer-btn span:first-child
{
    height: 39px;

    border-radius: 0 0 0 5px;
    background-color: #fff;
}
.layer-btn:before
{
    position: absolute;
    top: 0;
    left: 50%;

    width: 1px;
    height: 39px;

    content: ' ';

    background-color: #ebebeb;
}


.layer-fbtn
{
    font-size: 0;
    line-height: 30px;

    position: relative;

    display: -webkit-box;

    height: 30px;
    padding: 0 0 10px;

    text-align: center;
}
.layer-fbtn span
{
    display: block;

    height: 30px;

    border: 0;

            box-flex: 1;
    -webkit-box-flex: 1;
    /* margin:0 10px;*/
}

.layer-fbtn span .btn
{
    font-size: 16px;
    line-height: 30px;

    display: block;

    margin: 0 10px 0 10px;

    color: #fff;
    border-radius: 5px;
    background: #00aaef;
}

.layer-fbtn span:first-child .btn
{
    margin: 0 5px 0 10px;
}
.layer-fbtn span:last-child .btn
{
    margin: 0 10px 0 5px;
}
.layer-fbtn span:only-child .btn
{
    margin: 0 10px;
}
